#include <iostream>
using namespace std;

void intersection(int a[], int b[], int m,int n)
{
    int i=0, j=0;
    while (i<m && j<n)
    {
        if(i>0 && a[i]==a[i-1])
        {
            i++;
            continue;
        }
        if(a[i]<b[j])
        {
            i++;
        }
        else if(a[i]>b[j])
        {
            j++;
        }
        else
        {
            cout<< a[i]<< " ";
            i++;
            j++;
        }
    }
}

int main() {
    int arr1[] = {10,20,20,40,60};
    int arr2[] = {2,20,20,20};
    int m = sizeof(arr1) / sizeof(arr1[0]);
    int n = sizeof(arr2) / sizeof(arr2[0]);
    intersection(arr1, arr2, m,n);
    

    return 0;
}