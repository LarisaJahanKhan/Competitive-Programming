#include <iostream>
#include<algorithm>
using namespace std;
void printUnion (int a[], int b[], int m, int n)
{
    int i=0;
    int j=0;
    while(i<m&&j<n)
    {
       if(i>0 && a[i]==a[i-1])
       {
           i++;
           continue;
       }
       if(i>0 && b[j]==b[j-1])
       {
           j++;
           continue;
       }
       if(a[i]<b[j])
    {
        cout<<a[i]<<" ";
        i++;
    }
    else if (a[i]>b[j])
    {
        cout<<b[j]<<" ";
        j++;
    }
    else
    {
        cout<<a[i]<<" ";
        i++; 
        j++;
    }
    }
    while (i < m)
    {
        if (i == 0 || a[i] != a[i - 1])
            cout << a[i] << " ";
        i++;
    }

    while (j < n)
    {
        if (j == 0 || b[j] != b[j - 1])
            cout << b[j] << " ";
        j++;
    }

}
int main() {
    int array1[]={2, 3, 4, 4};
    int array2[]={5, 10, 20};
    int m = sizeof(array1) / sizeof(array1[0]);
    int n = sizeof(array2) / sizeof(array2[0]);
    printUnion(array1, array2, m,n);
    return 0;
}