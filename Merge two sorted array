#include <iostream>
using namespace std;
void merge (int a[], int b[], int m, int n)
{
    int i=0;
    int j=0;
    while(i<m&&j<n)
    {
       if(a[i]<=b[j])
    {
        cout<<a[i]<<" ";
        i++;
    }
    else
    {
        cout<<b[j]<<" ";
        j++;
    }
    }
    while (i<m)
    {
        cout<<a[i]<<" ";
        i++;
    }

    while (j<n)
    {
        cout<<b[j]<<" ";
        j++;
    }
    
}
int main() {
    int array1[]={3, 10, 2, 4};
    int array2[]={20, 5, 10};
    int m = sizeof(array1) / sizeof(array1[0]);
    int n = sizeof(array2) / sizeof(array2[0]);
    merge (array1, array2, m,n);
    return 0;
}